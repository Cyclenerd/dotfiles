{
    "telemetry.telemetryLevel":"off",
    "editor.minimap.enabled": false,
    "editor.renderWhitespace": "all",
    "editor.renderControlCharacters": true,
    "editor.fontSize": 16,
    "editor.fontFamily": "'Source Code Pro', Monaco, 'Courier New', monospace",
    "workbench.colorCustomizations": {
        "tab.border": "#1e1f1c",
        "tab.inactiveBackground": "#1e1f1c",
        "tab.inactiveForeground": "#ccccc7",
        "tab.activeBackground": "#414339",
        "tab.activeForeground": "#ccccc7"
    },
    "editor.acceptSuggestionOnEnter": "smart",
    "window.newWindowDimensions": "inherit",
    "workbench.startupEditor": "newUntitledFile",
    "extensions.ignoreRecommendations": true,
    "git.ignoreMissingGitWarning": true,
    "files.eol": "\n",
    "git.enableSmartCommit": true,
    "files.associations": {
        "*.tt2": "tt",
        "*.yaml": "yaml"
    },
    "workbench.editor.enablePreview": false,
    "problems.decorations.enabled": false,
    "html.autoClosingTags": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "remote.SSH.remotePlatform": {
        "debian": "linux"
    },
    "editor.fontLigatures": "'ccmp' off",
    "explorer.confirmDragAndDrop": false,
    "git.confirmSync": false,
    "security.workspace.trust.untrustedFiles": "open",
    "explorer.confirmDelete": false,
    "workbench.iconTheme": "vscode-icons",
    "git.openRepositoryInParentFolders": "never",
    "yaml.maxItemsComputed": 250000,
    "vsicons.dontShowNewVersionMessage": true,
    "workbench.colorTheme": "Dracula Theme",
    "git.autofetch": true,
    "terminal.integrated.fontSize": 16,
    "terminal.integrated.fontFamily": "Source Code Pro",
    "go.toolsManagement.autoUpdate": true,
    "cloudcode.duetAI.enable": false,
    "cloudcode.duetAI.inlineSuggestions.enableAuto": false,
    "dart.flutterSdkPath": "/Users/nils/flutter",
    "editor.stickyScroll.enabled": false,
    "yaml.schemaStore.enable": false,
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "dev.containers.dockerPath": "podman",
    "dev.containers.dockerComposePath": "pdoman-compose"
}