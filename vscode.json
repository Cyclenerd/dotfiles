{
    "telemetry.enableTelemetry": false,
    "editor.minimap.enabled": false,
    "editor.renderWhitespace": "all",
    "editor.renderControlCharacters": true,
    "editor.fontSize": 14,
    "editor.fontFamily": "'Source Code Pro Medium', 'Source Code Pro', Monaco, 'Courier New', monospace",
    "workbench.colorTheme": "Dracula",
    "workbench.colorCustomizations": {
        "tab.border": "#1e1f1c",
        "tab.inactiveBackground": "#1e1f1c",
        "tab.inactiveForeground": "#ccccc7",
        "tab.activeBackground": "#414339",
        "tab.activeForeground": "#ccccc7"
    },
    "editor.acceptSuggestionOnEnter": "smart",
    "window.newWindowDimensions": "inherit",
    "workbench.startupEditor": "newUntitledFile",
    "terminal.external.osxExec": "iTerm.app",
    "terminal.external.linuxExec": "lxterm",
    "extensions.ignoreRecommendations": true,
    "extensions.showRecommendationsOnlyOnDemand": true,
    "git.ignoreMissingGitWarning": true,
    "terminal.integrated.shell.windows": "C:\\Windows\\System32\\wsl.exe",
    "files.eol": "\n",
    "git.enableSmartCommit": true,
    "files.associations": {
        "*.tt2": "tt"
    },
    "workbench.editor.enablePreview": false,
    "window.zoomLevel": 0,
    "problems.decorations.enabled": false,
}